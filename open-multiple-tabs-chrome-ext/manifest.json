{
  "name": "Open Multiple Tabs",
  "version": "0.1",
  "description": "Opens a set of tabs with a press of a button",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "hammer.png",
    "default_popup": "index.html",
    "default_title": "Open multiple tabs"
  },
  "permissions": ["tabs"],
  "manifest_version": 2
}
